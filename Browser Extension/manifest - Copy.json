{
  "name": "TU anti-phishing",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension was for get the URL from Browser",
  "homepage_url": "http://localhost:8000/",
  "icons": {
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },

  "permissions": [
  "webNavigation",
    "tabs",
	"http://localhost:8000/*"
  ]
  
}