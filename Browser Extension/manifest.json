{
  "name": "TU anti-phishing",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "This extension was for get the URL from Browser",
  "homepage_url": "http://localhost:8000/",
  "icons": {
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
    "background": {
    "service_worker": "src/bg/background.js"
  },
  "action": {
        "default_popup": "src/browser_action/browser_action.html",
		"default_title": "browser action demo",
        "default_icon": {
            "128": "icons/icon128.png"
        }
    },


  "permissions": [
  "webNavigation",
    "tabs"
  ],
    "host_permissions": [
    "http://localhost:8000/*"
  ]
  
}